#fish aliases

alias st="git status"
alias com="git commit"
alias pus="git push"
alias pul="git pull"
alias cmakevim="cmake -DCMAKE_EXPORT_COMPILE_COMMANDS=ON"
alias ls='ls --file-type --color=auto --time-style=+"%d.%m.%Y-%H:%M"'
alias grep='grep --color=auto'
alias cp='cp -r'
alias vi='vim'
alias vd='vimdiff'
